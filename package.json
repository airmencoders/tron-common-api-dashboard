{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookstate/core": "^3.0.6",
    "@hookstate/initial": "^3.0.0",
    "@hookstate/touched": "^3.0.0",
    "@hookstate/validation": "^3.0.0",
    "@rehooks/local-storage": "^2.4.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@trussworks/react-uswds": "^1.12.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.14",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/swagger-ui-react": "^3.35.1",
    "@types/uuid": "^8.3.0",
    "@visx/group": "^2.1.0",
    "@visx/mock-data": "^2.1.0",
    "@visx/scale": "^2.1.0",
    "@visx/shape": "^2.1.1",
    "ag-grid-community": "^25.0.1",
    "ag-grid-react": "^25.0.1",
    "ajv": "^8.4.0",
    "apexcharts": "^3.26.0",
    "axios": "^0.21.4",
    "bootstrap": "^4.5.3",
    "bootstrap-icons": "^1.3.0",
    "chai": "^4.2.0",
    "d3-array": "^3.0.4",
    "date-fns": "^2.22.1",
    "fast-deep-equal": "^3.1.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "react": "^17.0.1",
    "react-apexcharts": "^1.3.9",
    "react-bootstrap": "^1.4.3",
    "react-copy-to-clipboard": "^5.0.3",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.12.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^7.0.4",
    "sass": "^1.32.8",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "swagger-ui-react": "^3.50.0",
    "ts-json-schema-generator": "^0.92.0",
    "typescript": "^4.1.3",
    "uswds": "^2.8.0",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npx react-scripts test",
    "eject": "react-scripts eject",
    "test:unit": "npm install && react-scripts test --coverage --watchAll=false --runInBand",
    "cypress:open": "npx cypress open --env INTEGRATION_TEST_HOST=http://localhost:3000",
    "test:e2e-local": "npx cypress run --env INTEGRATION_TEST_HOST=http://localhost:3000",
    "test:e2e-ci-local": "npx cypress run --env INTEGRATION_TEST_HOST=http://localhost:3000,ADMIN_API_HOST=http://localhost:3000,APP_CLIENT_API_HOST=http://localhost:3000,APP_CLIENT_DASHBOARD_API_HOST=http://localhost:3000",
    "test:e2e-ci": "npx cypress run --env INTEGRATION_TEST_HOST=https://tron-common-api.staging.dso.mil,ADMIN_API_HOST=https://tron-common-api.staging.dso.mil,APP_CLIENT_API_HOST=https://tron-common-api.staging.dso.mil,APP_CLIENT_DASHBOARD_API_HOST=https://tron-common-api.staging.dso.mil",
    "sonar": "sonar-scanner -Dsonar.projectVersion=${version}",
    "clean:generated-api-client": "rimraf src/openapi",
    "generate-api-client:code": "npm run clean:generated-api-client && openapi-generator-cli generate -g typescript-axios -i resources/tron-common-api.json -o src/openapi --additional-properties=withSeparateModelsAndApi=true,apiPackage=apis,modelPackage=models,withInterfaces=true",
    "generate-api-client:guards": "./node_modules/.bin/ts-json-schema-generator --path 'src/openapi/models/**/*.ts' -o src/api/model-types.json -i 'model-types.json'",
    "generate-api-client:guards-windows": ".\\node_modules\\.bin\\ts-json-schema-generator --path src/openapi/models/**/*.ts -o src/api/model-types.json -i model-types.json",
    "generate-api-client:post": "node process-json.js",
    "generate-api-client": "npm run generate-api-client:code && npm run generate-api-client:guards && npm run generate-api-client:post",
    "generate-api-client:windows": "npm run generate-api-client:code && npm run generate-api-client:guards-windows && npm run generate-api-client:post",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preinstall": "npx npm-force-resolutions"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.22",
    "@types/d3-array": "^3.0.1",
    "@types/jwt-simple": "^0.5.33",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-modal": "^3.10.6",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cypress": "^7.6.0",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jwt-simple": "^0.5.6",
    "msw": "^0.26.1",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/openapi/**/*",
      "!<rootDir>/src/stories/**/*"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!d3-(array|format))/"
    ]
  },
  "resolutions": {
    "remark-parse": "9.0.0",
    "prismjs": "1.24.0",
    "url-parse": "1.5.3",
    "tar": "6.1.7",
    "path-parse": "1.0.7",
    "axios": "0.21.4",
    "ansi-regex": "5.0.1",
    "shell-quote": "1.7.3",
    "json-schema": "0.4.0"
  }
}

    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@trussworks/react-uswds": "^1.10.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.14",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/uuid": "^8.3.0",
    "ag-grid-community": "^25.0.1",
    "ag-grid-react": "^25.0.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "bootstrap-icons": "^1.3.0",
    "chai": "^4.2.0",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-modal": "^3.12.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test:unit": "react-scripts test --coverage --watchAll=false",
    "cypress:open": "npx cypress open --env INTEGRATION_TEST_HOST=http://localhost:3000/",
    "test:e2e-local": "npx cypress run --env INTEGRATION_TEST_HOST=http://localhost:3000/",
    "test:e2e-ci": "npx cypress run --env INTEGRATION_TEST_HOST=https://tron-common-api.staging.dso.mil/ configFile=pipeline",
    "sonar": "sonar-scanner -Dsonar.projectVersion=${version}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-docs": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/node-logger": "^6.1.14",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.14",
    "@types/react-modal": "^3.10.6",
    "@types/react-router-dom": "^5.1.7",
    "cypress": "^6.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/openapi/**/*",
      "!<rootDir>/src/stories/**/*"
    ]
  }
}
